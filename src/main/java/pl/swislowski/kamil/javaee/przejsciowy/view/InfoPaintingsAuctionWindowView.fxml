               <children>
                  <Button fx:id="nextButton" mnemonicParsing="false" onAction="#nextButtonAction" text="Dalej" />
                  <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelAction" text="Zakończ" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
