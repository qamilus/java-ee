   <children>
      <VBox prefHeight="400.0" prefWidth="400.0">
         <children>
            <TextArea fx:id="infoTextArea" prefHeight="250.0" prefWidth="300.0" text="autor : Kamil Swislowski,&#10;przedmiot: Projekt przejściowy,&#10;kierunek studiów podypolomowych:  Java EE &#10;- produkcja oprogramowania,&#10;lista funkcjonalności :" />
